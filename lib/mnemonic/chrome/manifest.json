{
  "name": "Mnemonic",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Generate secure and memorable uids/passwords.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "mnemonic.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": ["clipboardWrite"]
}